  "scripts": {
    "lint": "npx xo",
    "test": "atom -t ./spec"
  },
  "version": "0.0.5",
  "xo": {
    "globals": [
      "Chai",
      "after",
      "afterEach",
      "atom",
      "attachToDOM",
      "before",
      "beforeEach",
      "describe",
      "document",
      "expect",
      "it",
      "when",
      "waitsForPromise",
      "window"
    ],
    "ignores": [
      "atom/**"
    ],
    "rules": {
      "no-constant-condition": "off",
      "yoda": [
        "error",
        "always",
        {
          "onlyEquality": false
        }
      ],
      "no-unused-vars": [
        "warn",
        {
          "args": "none"
        }
      ],
      "no-await-in-loop": "off",
      "import/prefer-default-export": "off",
      "max-nested-callbacks": "off",
      "camelcase": [
        "warn",
        {
          "allow": []
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "spec/*.js"
        ],
        "rules": {
          "no-unused-expressions": "off"
        }
      }
    ]
  }
